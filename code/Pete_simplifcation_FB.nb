a349a8388248"],

Cell[TextData[StyleBox["one could also do it directly from the \
non-approximated B, rho and un and vn (normalised eigenvectors)",
 Background->RGBColor[0.88, 1, 0.88]]], "Text",
 CellChangeTimes->{
  3.75654935271399*^9, {3.756549772780706*^9, 
   3.756549779505458*^9}},ExpressionUUID->"bb194af9-2dd4-48aa-b096-\
0cf547445ab0"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"B", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "2"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"un", "[", 
         RowBox[{"[", "i", "]"}], "]"}], 
        RowBox[{"(", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"j", "=", "1"}], "2"], 
          RowBox[{
           RowBox[{"vn", "[", 
            RowBox[{"[", "j", "]"}], "]"}], 
           RowBox[{"reproMatrix", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}]}]}], ")"}]}], ")"}]}], "+", 
     RowBox[{"rho", 
      RowBox[{"(", 
       RowBox[{"1", "-", "rho"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"j", "=", "1"}], "2"], 
        RowBox[{
         RowBox[{"un", "[", 
          RowBox[{"[", "j", "]"}], "]"}], 
         SuperscriptBox[
          RowBox[{"vn", "[", 
           RowBox[{"[", "j", "]"}], "]"}], "2"]}]}], ")"}]}]}]}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"exact\[Phi]", "=", 
   FractionBox[
    RowBox[{"2", 
     RowBox[{"(", 
      RowBox[{"rho", "-", "1"}], ")"}], "vn"}], "B"]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Assuming", "[", 
  RowBox[{"assumptions", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"Series", "[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"2", 
          RowBox[{"(", 
           RowBox[{"rho", "-", "1"}], ")"}], "vn"}], "B"], "/.", "approx"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"\[Epsilon]", ",", "0", ",", "1"}], "}"}]}], "]"}], "//", 
     "Normal"}], "//", "FullSimplify"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"-", "\[Phi]2"}], "+", "%"}], "//", 
  "Simplify"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.7565492264977627`*^9, 3.756549250741934*^9}, 
   3.756549295369151*^9, {3.756549349904727*^9, 3.7565493657445583`*^9}},
